# Example environment variables for Kalo CLI
# Copy this file to .env and customize as needed

# Base directories
MORPHE_BASE_DIR=/path/to/morphe/files

# Input directories
MORPHE_YAML_DIR=${MORPHE_BASE_DIR}/yaml
MORPHE_MODELS_DIR_YAML=${MORPHE_YAML_DIR}/models
MORPHE_ENTITIES_DIR_YAML=${MORPHE_YAML_DIR}/entities
MORPHE_ENUMS_DIR_YAML=${MORPHE_YAML_DIR}/enums
MORPHE_STRUCTURES_DIR_YAML=${MORPHE_YAML_DIR}/structures

# Output directories for Go
MORPHE_GO_DIR=${MORPHE_BASE_DIR}/go
MORPHE_MODELS_DIR_GO=${MORPHE_GO_DIR}/models
MORPHE_ENTITIES_DIR_GO=${MORPHE_GO_DIR}/entities
MORPHE_ENUMS_DIR_GO=${MORPHE_GO_DIR}/enums
MORPHE_STRUCTURES_DIR_GO=${MORPHE_GO_DIR}/structures

# Output directories for PostgreSQL
MORPHE_PSQL_DIR=${MORPHE_BASE_DIR}/psql
MORPHE_MODELS_DIR_PSQL=${MORPHE_PSQL_DIR}/models
MORPHE_ENTITIES_DIR_PSQL=${MORPHE_PSQL_DIR}/entities
MORPHE_ENUMS_DIR_PSQL=${MORPHE_PSQL_DIR}/enums

# Output directories for TypeScript
MORPHE_TS_DIR=${MORPHE_BASE_DIR}/ts
MORPHE_MODELS_DIR_TS=${MORPHE_TS_DIR}/models
MORPHE_ENTITIES_DIR_TS=${MORPHE_TS_DIR}/entities
MORPHE_ENUMS_DIR_TS=${MORPHE_TS_DIR}/enums
MORPHE_STRUCTURES_DIR_TS=${MORPHE_TS_DIR}/structures

# Plugin specific configurations
USE_BIGSERIAL=true
DEFAULT_SCHEMA=public
TS_MODE=strict
GENERATE_INTERFACES=true
LOG_LEVEL=info 